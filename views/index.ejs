    <!--==================== HEADER ====================-->
    <header class="header" id="header">
      <nav class="nav container">
        <a href="#" class="nav__logo">OZsum</a>

        <div class="nav__menu" id="nav-menu">
          <ul class="nav__list grid">
            <li class="nav__item">
              <a href="#home" class="nav__link active-link">
                <i class="uil uil-estate nav__icon"></i> Home
              </a>
            </li>
            <li class="nav__item">
              <a href="#about" class="nav__link">
                <i class="uil uil-user nav__icon"></i> About
              </a>
            </li>
            <li class="nav__item">
              <a href="#features" class="nav__link">
                <i class="uil uil-file-alt nav__icon"></i> Features
              </a>
            </li>
            <li class="nav__item">
              <a href="#services" class="nav__link">
                <i class="uil uil-briefcase-alt nav__icon"></i> Services
              </a>
            </li>
            <li class="nav__item">
              <a href="#contact" class="nav__link">
                <i class="uil uil-message nav__icon"></i> Contactme
              </a>
            </li>
          </ul>
          <i class="uil uil-times nav__close" id="nav-close"> </i>
        </div>
        <div class="nav__btns">
          <!-- Theme change button -->
          <i class="uil uil-moon change-theme" id="theme-button"></i>

          <div class="nav__toggle" id="nav-toggle">
            <i class="uil uil-apps"></i>
          </div>
        </div>
      </nav>
    </header>

    <!--==================== MAIN ====================-->
    <main class="main">
      <!--==================== HOME ====================-->
      <section class="home section" id="home">
        <div class="home__container container grid">
          <div class="home__content grid">
            <div class="home__social">
              <a
                href="https://au.linkedin.com/in/ozsum-accouting-and-business-services-aa685b205"
                target="_blank"
                class="home__social-icon"
              >
                <i class="uil uil-linkedin-alt"></i>
              </a>
              <a
                href="https://facebook.com/"
                target="_blank"
                class="home__social-icon"
              >
                <i class="uil uil-facebook-f"></i>
              </a>
              <a
                href="https://twitter.com/"
                target="_blank"
                class="home__social-icon"
              >
                <i class="uil uil-twitter-alt"></i>
              </a>
            </div>
            <div class="home__img">
               <img src="/assets/OZsumlogo.png" alt="">
            </div>
            <div class="home__data">
              <h1 class="home__title">OZsum</h1>
              <!-- <h3 class="home__subtitle">KPO Firm</h3> -->
              <p class="home__description">
                High level experience - development knowledge -
                producing quality work.
              </p>
              <a href="#contact" class="button button--flex">
                Contact Us
                <i class="uil uil-message button__icon"> </i>
              </a>
            </div>
          </div>
          <div class="home__scroll">
            <a href="#about" class="home__scroll-button button--flex">
              <i class="uil uil-mouse-alt home__scroll-mouse"></i>
              <span class="home__scroll-name"> Scroll down </span>
              <i class="uil uil-arrow-down home__scroll-arrow"></i>
            </a>
          </div>
        </div>
      </section>

      <!--==================== ABOUT ====================-->
      <section class="about section" id="about">
        <h2 class="section__title">About Our Company</h2>
        <!-- <span class="section__subtitle">My introduction</span> -->

        <div class="about__container container grid">
          <!-- <img src="assets/img/about.jpg" alt="" class="about__img" /> -->

          <div class="about__data">
            <p class="about__description">
                    OZsum is a start-up Knowledge Process Outsourcing (KPO) firm endeavored to improve the finance and accounting functions of your organization. We are well equipped with professionally qualified and experienced members with sound knowledge in advanced and state-of-the-art technology platforms, who would understand your needs and devise cost-effective solutions accordingly. This lets you focus on the core and strategic activities of your business and thereby save precious time and money; further, it alleviates the burden of having to hire accounting professionals and invest heavily on technology.
            </p>
          </div>
        </div>
      </section>


      <!--==================== SERVICES ====================-->
      <section class="services section" id="services">
        <h2 class="section__title">Services</h2>
        <span class="section__subtitle">What we offer</span>

        <div class="services__container container grid">
          <!-- services 1 -->
          <div class="services__content">
            <div>
              <i class="uil uil-web-grid services__icon"></i>
              <h3 class="services__title">
                Accounting & <br />
                Taxation
              </h3>
            </div>
          </div>

          <!-- services 2 -->
          <div class="services__content">
            <div>
              <i class="uil uil-clipboard-alt services__icon"></i>
              <h3 class="services__title">
                Payroll & <br />
                Bookkeeping
              </h3>
            </div>
          </div>

          <!-- services 3 -->
          <div class="services__content">
            <div>
              <i class="uil uil-pen services__icon"></i>
              <h3 class="services__title">
                Preparation of<br />
                BAS & IAS
              </h3>
            </div>
          </div>

          <!-- services 3 -->
          <div class="services__content">
            <div>
              <i class="uil uil-book-reader services__icon"></i>
              <h3 class="services__title">
                Preparation of<br />
                Management Reports
              </h3>
            </div>
          </div>


          <!-- services 3 -->
          <div class="services__content">
            <div>
              <i class="uil uil-building services__icon"></i>
              <h3 class="services__title">
                Business Services
              </h3>
            </div>
          </div>


          <!-- services 3 -->
          <div class="services__content">
            <div>
              <i class="uil uil-comparison services__icon"></i>
              <h3 class="services__title">
                Preparation of<br />
                Financial Reports
              </h3>
            </div>
          </div>
        </div>
      </section>

      <!--==================== features ====================-->
      <section class="features section" id="features">
        <h2 class="section__title">Features</h2>
        <!-- <span class="section__subtitle">Most recent work</span> -->

        <div class="features__container container swiper-container">
          <div class="swiper-wrapper">
            <!-- features 1 -->
            <div class="features__content flex swiper-slide">
                <div class="item">
                    <i class="fas fa-user-friends"></i>
                    <p>Team of qualified and experienced professionals</p>
                </div>
                <div class="item">
                    <i class="fas fa-space-shuttle"></i>
                    <p>Sound,effective and organised workflow</p>
                </div>
            </div>

            <!-- features 2 -->
            <div class="features__content flex swiper-slide">
                <div class="item">
                    <i class="fas fa-industry"></i>
                    <p>Offers real-time insights through on-demand reports and intelligence through predictive models</p>
                </div>
                <div class="item">
                    <i class="fas fa-database"></i>
                    <p>Data-driven decisions for your business</p>
                </div>
            </div>

            <!-- features 3 -->
            <div class="features__content flex swiper-slide">
                <div class="item">
                    <i class="fas fa-hands-helping"></i>
                    <p>Helps to mitigate risks and improve compliance</p>
                </div>
                <div class="item">
                    <i class="fas fa-file"></i>
                    <p>Well-documented Data Security Measures</p>
                </div>
            </div>

            <!-- features 4 -->
            <div class="features__content flex swiper-slide">
                <div class="item">
                    <i class="fas fa-search-minus"></i>
                    <p>Well-established KPI’s (Key Performance Indicators) to monitor process performance</p>
                </div>
                <div class="item">
                    <i class="fas fa-dollar-sign"></i>
                    <p>Nominal pricing based on a “What-You-Use is What-You-Pay” basis</p>
                </div>
            </div>
          </div>

          <div class="swiper-button-next">
            <i class="uil uil-angle-right-b swiper-features-icon"></i>
          </div>
          <div class="swiper-button-prev">
            <i class="uil uil-angle-left-b swiper-features-icon"></i>
          </div>

          <div class="swiper-pagination"></div>
        </div>
      </section>

      <!--==================== tools ====================-->
      <section class="tools section">
        <h2 class="section__title">Tools</h2>
        <!-- <span class="section__subtitle">My client saying</span> -->

        <div class="tools__container container swiper-container">
          <div class="swiper-wrapper">
            <!-- tools 1 -->
            <div class="tools__content swiper-slide">
              <div class="tool-img"><img src="/assets/app1.png" /></div>
            </div>

            <!-- tools 2 -->
            <div class="tools__content swiper-slide putdown">
              <div class="tool-img"><img src="/assets/app3.png" /></div>
            </div>
            <!-- tools 2 -->
            <div class="tools__content swiper-slide putdown">
              <div class="tool-img"><img src="/assets/app2.png" /></div>
            </div>
            <!-- tools 2 -->
            <div class="tools__content swiper-slide putdown">
              <div class="tool-img"><img src="/assets/app4.png" /></div>
            </div>
            <div class="tools__content swiper-slide">
              <div class="tool-img"><img src="/assets/app5.png" /></div>
            </div>
          </div>
          <!-- add pagination  -->
          <div class="swiper-pagination swiper-pagination-tools"></div>
        </div>
      </section>

      <!--==================== CONTACT ME ====================-->
      <section class="contact section" id="contact">
        <h2 class="section__title">Contact Us</h2>
        <span class="section__subtitle">Get in touch</span>

        <div class="contact__container container grid">
          <div>
            <div class="contact__information">
              <i class="uil uil-phone contact__icon"></i>

              <div>
                <h3 class="contact__title">Call Us</h3>
                <span class="contact__subtitle">+61 414 411 992</span>
              </div>
            </div>

            <div class="contact__information">
              <i class="uil uil-envelope contact__icon"></i>

              <div>
                <h3 class="contact__title">Email</h3>
                <span class="contact__subtitle">ozsum.services@outlook.com.au</span>
              </div>
            </div>
          </div>

          <form action="/mail" class="contact__form grid" method="POST">
            <div class="contact__inputs grid">
              <div class="contact__content">
                <label for="name" class="content__label">Name</label>
                <input type="text" class="contact__input" name="name">
                <div id="fname"></div>
              </div>
              <div class="contact__content">
                <label for="email" class="content__label">Email</label>
                <input type="email" class="contact__input" name="email">
                <div id="femail"></div>
              </div>
            </div>
            <div class="contact__content">
              <label for="message" class="content__label">Message</label>
              <textarea name="message" id="" cols="0" rows="7" class="contact__input"></textarea>
              <div id="fmessage"></div>
            </div>

            <div class="form__button">
              <button id="formbutton" type="submit" class="button button--flex">
                Send Message
                <i class="uil uil-message button__icon"></i>
              </button>
            </div>
            <div class="form__message" id="form__message">
              <h3>Thank you, we've received your message.</h3>
            </div>
          </form>
        </div>
      </section>
    </main>

